<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="view" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.0" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="experienciaTable" editable="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="desdeColumn" prefWidth="75.0" text="Desde" />
          <TableColumn fx:id="hastaColumn" prefWidth="75.0" text="Hasta" />
            <TableColumn fx:id="denominacionColumn" minWidth="100.0" prefWidth="75.0" text="Denominación" />
            <TableColumn fx:id="empleadorColumn" minWidth="100.0" prefWidth="75.0" text="Empleador" />
        </columns>
      </TableView>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0">
         <children>
            <Button fx:id="addExperienciaButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onAddExperienciaButtonAction" text="Añadir" />
            <Button fx:id="eliminarExperienciaButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onEliminarExperienciaButtonAction" text="Eliminar" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
